<div class="container">
  <div class="row">
    <div class="col-md-12">
      <%= render 'users/wellcome' %>
    </div>

    <% @users.each do |user| %>
      <div class="col-md-3 ">
        <%= image_tag (user.avatar_thumbnail), style: 'height:30px;width:30px;'%>
        <%= link_to "#{user.name}", user %>
        <%= user.email %>
      </div>
    <% end %>
  </div>
</div>
